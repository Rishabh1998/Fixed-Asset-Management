
<div class="container">

  <%= form_for :category, url:admin_categories_path do |f| %>

    <%= f.label 'Department_id' %>
    <%= f.text_field :department_id %>

    <%= f.label 'Name' %>
    <%= f.text_field :name %>


    <%= f.label 'Description' %>
    <%= f.text_field :description %>


    <%= f.label 'Status' %>
    <%= f.text_field :status %>


    <%= f.submit %>

  <% end %>
</div>
